{
	"version": 2,
	"builds": [
		{
			"src": "/index.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "(.*)",
			"dest": "/index.ts"
		}
	],
	"env": {
		"NODE_ENV": "production",
		"PORT": "5000",
		"ALLOWED_CLICKS": "20",
		"BASE_URL": "https://url-shortner-kappa.vercel.app/api/v1"
	}
}